<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
    
    <servlet>
        <servlet-name>empresa</servlet-name>
        <servlet-class>com.analixdata.controladores.EmpresaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>empresa</servlet-name>
        <url-pattern>/empresa</url-pattern>
    </servlet-mapping>
 
    <servlet>
        <servlet-name>validar</servlet-name>
        <servlet-class>com.analixdata.controladores.ValidarServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>validar</servlet-name>
        <url-pattern>/validar</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>servicio</servlet-name>
        <servlet-class>com.analixdata.controladores.ServicioServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servicio</servlet-name>
        <url-pattern>/servicio</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>usuario</servlet-name>
        <servlet-class>com.analixdata.controladores.UsuarioServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>usuario</servlet-name>
        <url-pattern>/usuario</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>cerrarSesion</servlet-name>
        <servlet-class>com.analixdata.controladores.CerrarSesionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>cerrarSesion</servlet-name>
        <url-pattern>/cerrarSesion</url-pattern>
    </servlet-mapping>
    
    
    
    <servlet>
        <servlet-name>enviarSMS</servlet-name>
        <servlet-class>com.analixdata.controladores.EnvioMensajesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>enviarSMS</servlet-name>
        <url-pattern>/enviarSMS</url-pattern>
    </servlet-mapping>
    
    
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
</web-app>